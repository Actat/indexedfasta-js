{
  "presets": [
    ["env", {
      "targets": {
        "node": 6,
        "browsers": [
          "> 0.5%",
          "last 2 versions"
        ]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": false,
      "polyfill": true,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ]
}
